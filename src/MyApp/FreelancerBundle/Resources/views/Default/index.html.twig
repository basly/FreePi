{% extends '@Freelancer/Default/layout.html.twig' %}

{%  block content %}
    <div class="col-lg-offset-1 col-md-2">
    <div class="about w3layouts-agileinfo" id="about">
        <ul class="list-group w3-agile">
            <li class="list-group-item"><h4>Freelancer</h4></li>
            {% for user in freelancers %}
                <li class="list-group-item"><span>{{ user.username|e }} <a class="button18" href="{{ path('freelancer_Showfreelancer',{'email':user.email}) }}"><div>Profil</div></a></span></li>
            {% endfor %}
        </ul>
    </div>
    </div>
    <div class="col-md-offset-1 col-md-2">
        <div class="about w3layouts-agileinfo" id="about">
            <ul class="list-group w3-agile">
                <li class="list-group-item"><h4>JobOwner</h4></li>
                {% for user in Jobowners %}
                    <li class="list-group-item"><span>{{ user.username|e }} <a class="button18" href="{{ path('freelancer_Showjobowner',{'email':user.email}) }}"><div>Profil</div></a></span></li>
                {% endfor %}
            </ul>
        </div>
    </div>
{%  endblock %}