{% extends '@Freelancer/Default/training.html.twig' %}

{% block table %}
    <h1 class="display-1">Subscriptions List </h1>
<div class="bs-docs-example" align="center">
    <div style="text-align: center;font-size: 19px;color: grey;"></div>
        <thead>
            <tr>
                <th>Subscriber Last Name</th>
                <th>Subscriber First Name</th>
                <th>email</th>
            </tr>
        </thead>
        <tbody>
        {% for subscription in subscriptions %}
            <tr>
                <td><a href="{{ path('subscription_show', { 'id': subscription.id }) }}">{{ subscription.id }}</a></td>
                <td>{{ subscription.nom}}</td>
                <td>{{ subscription.prenom }}</td>
                <td>{{ subscription.email}}</td>
                <td> <a href="{{ path('subscription_show', { 'id': subscription.id }) }}">show</a></td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
    </div>


{% endblock %}
